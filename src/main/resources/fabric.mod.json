{
	"schemaVersion": 1,
	"id": "zenith_attributes",
	"version": "${version}",
	"name": "Zenith Attributes",
	"description": "Adds attributes, needed for Zenith",
	"authors": [
		"ShadowsOfFire (Forge)",
		"TheWinABagel (Fabric Port)"
	],
	"contact": {
		"sources": "https://github.com/TheWinABagel/ZenithAttributes"
	},
	"license": "CC0-1.0",
	"icon": "assets/zenith_attributes/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.shadowsoffire.attributeslib.AttributesLib"
		],
		"client": [
			"dev.shadowsoffire.attributeslib.client.AttributesLibClient"
		],
		"mixinsquared": [
			"dev.shadowsoffire.attributeslib.mixin.ZenithAttributesMixinCanceller"
		],
		"cardinal-components": [
			"dev.shadowsoffire.attributeslib.components.ZenithAttributesComponents"
		],
		"emi": [
			"dev.shadowsoffire.attributeslib.compat.EMICompat"
		],
		"rei_client": [
			"dev.shadowsoffire.attributeslib.compat.REICompat"
		]
	},
	"custom": {
		"cardinal-components": [
			"zenith_attributes:arrow_velocity_done",
			"zenith_attributes:arrow_damage_done"
		]
	},
	"mixins": [
		"zenith_attributes.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fakerlib": ">=0.1.3"
	},
	"suggests": {
		"zenith": "*"
	},
    "breaks": {
        "artifacts": "<9.4.0"
    },
	"accessWidener": "zenith_attributes.aw"
}